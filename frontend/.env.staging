# 预发布环境配置
NODE_ENV=staging
VITE_NODE_ENV=staging

# API 基础地址
VITE_API_BASE_URL=http://localhost:8000

# 应用标题
VITE_APP_TITLE=闪电推送系统 - 预发布环境

# 调试模式
VITE_DEBUG=false

# 应用版本
VITE_APP_VERSION=1.0.0-staging

# 环境标识
VITE_ENV_NAME=staging
VITE_ENV_DISPLAY_NAME=预发布环境

# 日志级别 (debug, info, warn, error)
VITE_LOG_LEVEL=warn

# 是否启用 Mock 数据
VITE_USE_MOCK=false

# WebSocket 地址
VITE_WS_BASE_URL=ws://localhost:8000

# 文件上传大小限制 (MB)
VITE_UPLOAD_SIZE_LIMIT=50

# 是否显示环境标识
VITE_SHOW_ENV_TAG=true

# 开发工具
VITE_DEV_TOOLS=false

# API 超时时间 (ms)
VITE_API_TIMEOUT=15000

# 是否启用性能监控
VITE_ENABLE_PERFORMANCE=true

# CDN 地址
VITE_CDN_URL=https://cdn-staging.example.com
